RewriteEngine On
RewriteCond %{HTTPS} off
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI}
# ======== TERMINA AQUI ========
<Files 403.shtml>
order allow,deny
allow from all
</Files>